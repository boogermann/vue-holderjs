{
	"name": "vue-holderjs",
	"version": "1.0.2",
	"description": "A Vue.js directive for HolderJS",
	"main": "dist/vue-holder.common.js",
	"unpkg": "dist/vue-holder.browser.js",
	"scripts": {
		"build": "npm run build:browser && npm run build:common",
		"build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
		"build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules"
	},
	"keywords": [
		"vue",
		"vue.js",
		"vue-holder",
		"vue-holderjs",
		"holderjs"
	],
	"author": "Marcus Andrade <marcusjordan@outlook.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/boogermann/vue-holderjs"
	},
	"bugs": {
		"url": "https://github.com/boogermann/vue-holderjs/issues"
	},
	"homepage": "https://github.com/boogermann/vue-holderjs#readme",
	"dependencies": {
		"holderjs": "^2.9.4",
		"jquery": "^3.2.1"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.1",
		"cross-env": "^5.1.1",
		"webpack": "^3.8.1",
		"webpack-merge": "^4.1.1"
	}
}
